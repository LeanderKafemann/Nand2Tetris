/**
 * Subtract gate:
 * out = a-b 
 */
CHIP Subtract16 {
    IN a[16], b[16];
    OUT out[16];
    
    PARTS:

Negate16(in = b , out = c);
Add16(a = a , b = c , out = out);

}
