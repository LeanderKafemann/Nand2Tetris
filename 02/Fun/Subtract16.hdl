/**
 * 16-bit subtractor: subtracts two 16-bit two's complement values.
 * The most significant carry bit is ignored.
 */
CHIP Subtract16 {
    IN a[16], b[16];
    OUT out[16];

    PARTS:
Not(in=a[15] , out a15);
And(a=a15 , b=b[15] , out=c1)

}
